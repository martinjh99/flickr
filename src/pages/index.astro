---
import Layout from '../layouts/Base.astro';
import { Debug } from 'astro:components'; 
const response = await fetch('https://www.flickr.com/services/rest/?method=flickr.photos.getPopular&api_key=e1cdbef9e8f75963062c57a7636856fc&user_id=83528065%40N00&extras=url_w%2Curl_o&format=json&nojsoncallback=1');
const photos = await response.json();
---
<Layout>
    <Debug {photos}/>
</Layout>
